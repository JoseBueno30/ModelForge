model BankAccount

enum AccountType {Investments, Savings}

class Account
attributes
balance : Integer
type : AccountType
operations
transfer(variable : Integer) : Integer begin
        self.balance := self.balance + 1;
    end
constraints
inv Positive_Balance:
	self.balance>0
end

class Bank
end

class Person
attributes
age : Integer
end

constraints
context x, y: Account inv Prueba:
	y.balance>x.balanceimpliesx.balance<y.balance

